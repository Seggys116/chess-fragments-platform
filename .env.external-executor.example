# External Executor Configuration
# Copy this file to .env and fill in the values

# Main server IP address (the machine running docker-compose.yml)
MAIN_SERVER_IP=192.168.1.100

# Credentials (must match main server .env)
POSTGRES_PASSWORD=your_postgres_password
REDIS_PASSWORD=your_redis_password

# Executor settings
EXECUTOR_CONCURRENCY=8           # Number of concurrent threads per executor
MATCHES_PER_EXECUTOR=4           # Matches this executor can handle
EXECUTOR_REPLICAS=1              # Number of executor containers to run
EXECUTOR_HEARTBEAT_INTERVAL=10   # Seconds between heartbeats

# Agent limits (should match main server)
AGENT_TIMEOUT_SECONDS=14
AGENT_MEMORY_LIMIT_MB=512

# Tournament mode (should match main server)
TOURNAMENT_NOW=false
